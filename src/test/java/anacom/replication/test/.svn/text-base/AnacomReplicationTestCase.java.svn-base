package anacom.replication.test;

import junit.framework.TestCase;

/**
 * jUnit tests parent class for Anacom's distributed replication testing.
 */
public abstract class AnacomReplicationTestCase extends TestCase {
	
	protected final static int YEAR = 0;
	protected final static int MONTH = 1;
	protected final static int DAY = 1;
	protected final static int HOUR = 0;
	protected final static int MINUTE = 0;
	protected final static int SECOND = 0;

	protected AnacomReplicationTestCase() { super(); }
	
	protected AnacomReplicationTestCase(String msg) { super(msg); }
	
}
