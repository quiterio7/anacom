<?xml version="1.0"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
 <context-param>
    <param-name>replicaNumber</param-name>
    <param-value>1</param-value>
 </context-param>
 <context-param>
  	<param-name>operator</param-name>
  	<param-value>Vodafone</param-value>
 </context-param>
 <context-param>
 	<param-name>prefix</param-name>
  <param-value>91</param-value>
 </context-param>
 <context-param>
 	<param-name>smsCost</param-name>
  <param-value>11</param-value>
 </context-param>
   <context-param>
 	<param-name>voiceCost</param-name>
  <param-value>10</param-value>
 </context-param>
  <context-param>
 	<param-name>videoCost</param-name>
  <param-value>10</param-value>
 </context-param>
  <context-param>
 	<param-name>tax</param-name>
  <param-value>10</param-value>
 </context-param>
  <context-param>
 	<param-name>bonus</param-name>
  <param-value>10</param-value>
 </context-param>
 <context-param>
    <param-name>pathAsymKeys</param-name>
        <param-value>
            RSAkeys/OP1keys
        </param-value>
    </context-param>
 <context-param>
    <param-name>CAPublicKey</param-name>
        <param-value>
            RSAkeys/CAkeys
        </param-value>
 </context-param>
 <listener>
  <description>Starts fenix framework upon webapp deployment.</description>
  <listener-class>anacom.applicationserver.ApplicationServerInitListener</listener-class>
 </listener>
 <servlet>
  <servlet-name>anacom</servlet-name>
  <servlet-class>anacom.applicationserver.ApplicationServerWebService</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>anacom</servlet-name>
  <url-pattern>/AnacomApplicationServer</url-pattern>
 </servlet-mapping>
</web-app>